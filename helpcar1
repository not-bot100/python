command=""
while command !="quit":
    command =input(">").lower()
    if command == "start":
        print("The car started.")
    elif command == "stop":
        print("Stop the car")
    elif command == "help":
        print("""
Start= Starting the car
Stop= stopping the car
quit= quit the race""")
    elif command == "quit":
        print("Quit the race")
        break
    else:
        print("no understand ")


